{
  "name": "Wallet",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "favourite": {
      "type": "boolean"
    },
    "idPagamentoFromEC": {
      "type": "string"
    },
    "lastUsage": {
      "type": "date",
      "required": true
    },
    "matchedPsp": {
      "type": "boolean"
    },
    "pspEditable": {
      "type": "boolean"
    },
    "type": {
      "type": "string",
      "default": "CREDIT_CARD"
    }
  },
  "validations": [],
  "relations": {
    "transactions": {
      "type": "hasMany",
      "model": "Transaction",
      "foreignKey": ""
    },
    "creditCard": {
      "type": "embedsOne",
      "model": "CreditCard",
      "property": "CreditCard",
      "options": {
        "validate": true,
        "forceId": false,
        "persistent": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
