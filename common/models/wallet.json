      "type": "string",
      "required": true
    },
    "lastUsage": {
      "type": "date"
    },
    "owner": {
      "type": "string"
    },
    "expires": {
      "type": "date",
      "required": true
    },
    "tag": {
      "type": "string"
    },
    "kind": {
      "type": "string",
