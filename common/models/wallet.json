{
  "name": "Wallet",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "number": {
      "type": "string",
      "required": true
    },
    "lastUsage": {
      "type": "date"
    },
    "expires": {
      "type": "date",
      "required": true
    },
    "tag": {
      "type": "string"
    },
    "kind": {
      "type": "string",
      "default": "Credit Card"
    }
  },
  "validations": [],
  "relations": {
    "transactions": {
      "type": "hasMany",
      "model": "Transaction",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
