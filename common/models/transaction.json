{
  "name": "Transaction",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "created": {
      "type": "date",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "error": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "idPayment": {
      "type": "number"
    },
    "idStatus": {
      "type": "number"
    },
    "merchant": {
      "type": "string"
    },
    "nodoIdPayment": {
      "type": "string"
    },
    "paymentModel": {
      "type": "number"
    },
    "statusMessage": {
      "type": "string"
    },
    "success": {
      "type": "boolean",
      "default": true
    },
    "updated": {
      "type": "date"
    },
    "urlCheckout3ds": {
      "type": "string"
    },
    "urlRedirectPsp": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "amount": {
      "type": "embedsOne",
      "model": "Amount",
      "property": "Amount",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "fee": {
      "type": "embedsOne",
      "model": "Amount",
      "property": "Fee",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "grandTotal": {
      "type": "embedsOne",
      "model": "Amount",
      "property": "GrandTotal",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
